@page "/terminy"
@model Korepetycje_Matematyka.Pages.TerminyModel
@{
}
<h1>Dostępne terminy korepetycji</h1><hr /><hr />
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Poniedziałek</th>
            <th>Wtorek</th>
            <th>Środa</th>
            <th>Czwartek</th>
            <th>Piątek</th>
        </tr>
    </thead>
    <tbody>
        @for(int i = 9; i<=20; i++)
        {   
            <tr>
            <td>@i.ToString();</td>
             <td>@i.ToString();</td>
              <td>@i.ToString();</td>
               <td>@i.ToString();</td>
                <td>@i.ToString();</td>
            </tr>
        }
    </tbody>
    </table>